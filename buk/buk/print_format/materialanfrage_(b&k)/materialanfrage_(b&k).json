 "html": "{% set project = \"n/a\" %}\n{% if doc.items|length > 0 %}\n    {% if doc.items[0].sales_order %}\n        {% set project = frappe.get_doc(\"Sales Order\", doc.items[0].sales_order).objekt or \"n/a\" %}\n    {% else %}\n        {% if doc.items[0].quotation %}\n            {% set project = doc.items[0].quotation %}\n        {% endif %}\n    {% endif %}\n{% endif %}\n<div id=\"header-html\" class=\"hidden-pdf\">\n    <img class=\"pull-right\" src=\"/assets/buk/img/braun_kloeti.jpg\" width=\"180\">\n    <table style=\"border: 1px solid black; background-color: #efefef; width: 100%;\">\n        <tr>\n            <td> <b>Materialanfrage<br></b></td>\n            <td>{{ doc.name }}</td>\n            <td><b>Seite</b></td>\n            <td><span class=\"page\"></span> / <span class=\"topage\"></span></td>\n        </tr>\n        <tr>\n            <td><b>Projekt</b></td>\n            <td>{{ project }}</td>\n            <td><b>Datum:</b></td>\n            <td>{{ doc.get_formatted('transaction_date') }}</td>\n        </tr>\n    </table>\n</div>\n<div style=\"margin-top:40px;\">\n    <table>\n        <thead style=\"font-weight:bold; background-color: #efefef; font-size:13px;\">\n            <td>Menge</td>\n            <td>Einheit</td>\n            <td>Artikel/Beschreibung</td>\n            <td>Artikelnummer</td>\n            <td>Termin</td>\n        </thead>\n        <tbody>\n            {% for item in doc.items %}\n                <tr>\n                    <td class=\"right\">{{ item.qty }}</td>\n                    <td>{{ item.uom }}</td>\n                    <td>{{ item.item_name }}<br></td>\n                    <td class\"right\">{{ item.item_code }}</td>\n                    <td>{{ frappe.utils.get_datetime(item.schedule_date).strftime('%d.%m.%Y') }}</td>\n                </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n</div>",
